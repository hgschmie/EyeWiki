    <attribute>
       <name>format</name>
    </attribute>
    <attribute>
       <name>version</name>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>newVersion</name>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>EditLink</name>
    <tagclass>de.softwareforge.eyewiki.tags.EditLinkTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <attribute>
       <name>page</name>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
       <name>format</name>
    </attribute>
    <attribute>
       <name>version</name>
       <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>

  <tag>
    <name>Editor</name>
    <tagclass>de.softwareforge.eyewiki.tags.EditorTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <attribute>
       <name>submit</name>
       <rtexprvalue>true</rtexprvalue>
